<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report.external_layout_header">
        <div class="header" style="border-bottom: 1px solid #0066FF; padding-bottom: 5px; width: 820px;">
            <table>
                <tr>
                    <td width="250">
                        <img style="max-height: 60px;" t-att-src="'data:image/png;base64,%s' % company.logo" t-if="company.logo"/>
                    </td>
                    <td style="color: #303030; vertical-align: top; padding-top: 10px; font-size: 10pt;" width="250">
                        <span t-if="o.company_id.name">
                            <span t-field="o.company_id.name"/>
                            <br/>
                        </span>
                        <span t-if="o.company_id.street">
                            <span t-field="o.company_id.street"/>
                            <br/>
                        </span>
                        <span t-if="o.company_id.street2">
                            <span t-field="o.company_id.street2"/>
                            <br/>
                        </span>
                        <span t-if="o.company_id.zip">
                            <span t-field="o.company_id.zip"/>
                        </span>
                        <span t-if="o.company_id.city">
                            <span t-field="o.company_id.city"/>
                            <br/>
                        </span>
                        <span t-if="o.company_id.country_id.code != 'CH'">
                            <span t-field="o.company_id.country_id.name"/>
                        </span>
                    </td>
                    <td style="color: #303030; vertical-align: top; padding-top: 10px; font-size: 10pt;" width="200">
                        <span t-if="o.company_id.phone">
                            <span style="display: inline-block; width: 20px;">✆</span>
                            <span t-field="o.company_id.phone"/>
                            <br/>
                        </span>
                        <span t-if="o.company_id.email">
                            <span style="display: inline-block; width: 20px;">✉</span>
                            <span t-field="o.company_id.email"/>
                            <br/>
                        </span>
                        <span t-if="o.company_id.website">
                            <span style="display: inline-block; width: 20px;">۞</span>
                            <span t-field="o.company_id.website"/>
                            <br/>
                        </span>
                    </td>
                    <td align="right">
                        <img src="data:image/png;base64," style="max-height: 60px;"/>
                    </td>
                </tr>
            </table>
        </div>
    </template>
</odoo>